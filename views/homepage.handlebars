{{#if nickname}}
<h2>{{nickname}}'s Posts</h2>
{{else}}
{{#if tag}}
<h2>{{tag}} Posts</h2>
{{/if}}
{{/if}}
{{#each posts}}
<article>
  <p>Post ID: {{id}}</p>
  <p>Title: <a id="titttle" href="/post/{{id}}">{{title}}</a></p>
  <p>Author: <a href="/user/{{user.post_author_id}}">{{user.post_author}}</a></p>
  <p>Description: {{description}}</p>
  <p>Coordinates: ({{latitude}}, {{longitude}})</p>
  {{#if image_url_sized}}
  <p><img src="{{image_url_sized}}" /></p>
  {{/if}}
  <p>Image URL: [{{image_url}}]</p>
    {{> tags this}}
  <p>Created at: {{created_at}}</p>
    {{> like-controls this}}
  <p><a href="/post/{{id}}">{{comment_count}} comments</a></p>
</article>
<br><br>
{{/each}}
<script src="/javascript/vote.js"></script>